msgid "Deskew"
msgstr ""

#: lib/Gscan2pdf/Unpaper.pm:274
msgid "Border"
msgstr "Granica"

#: lib/Gscan2pdf/Unpaper.pm:331
msgid "Filters"
msgstr "Filtry"
